{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "outDir": "./dist",
        "sourceMap": true,
        "declaration": false,
        "strictNullChecks": false,
        "noImplicitAny": false,
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "importHelpers": true,
        "target": "es2019",
        "typeRoots": ["node_modules/@types"],
        "lib": ["es2019"]
    }
}
